<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="1dip" android:color="#0106D5" />
            <gradient android:angle="-90" android:startColor="#0106D5" android:endColor="#0106D5"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="1dip" android:color="#0106D5" />
            <solid android:color="#0106D5"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="1dip" android:color="#0106D5" />
            <gradient android:angle="-90" android:startColor="#74FE3B" android:endColor="#74FE3B" />
        </shape>
    </item>
</selector>
